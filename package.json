    "less-loader": "^5.0.0",
    "mocha": "7.1.1",
    "mocha-babel": "3.0.3",
    "node-sass": "4.13.1",
    "react-hot-loader": "4.12.20",
    "react-redux": "7.2.0",
    "react-router": "5.1.2",
    "react-router-dom": "5.1.2",
    "react-router-redux": "^4.0.8",
    "sass-loader": "8.0.2",
