{
  "name": "react-big-scheduler",
  "version": "0.2.7",
  "description": "A React Typescript component for scheduling and made for modern browsers (IE10+).",
  "keywords": [
    "scheduler",
    "react-component",
    "react",
    "react-big-scheduler",
    "resource planning",
    "typescript"
  ],
  "files": [
    "lib/",
    "LICENSE",
    "README.md",
    "CHANGELOG.md"
  ],
  "main": "lib/index",
  "homepage": "https://github.com/graspea/react-typescript-scheduler",
  "author": "Graspea (https://github.com/graspea),Stephen Chou <27788280@qq.com> (https://github.com/StephenChou1017)",
  "repository": {
    "type": "git",
    "url": "git@github.com:graspea/react-typescript-scheduler.git"
  },
  "bugs": {
    "url": "https://github.com/graspea/react-typescript-scheduler/issues"
  },
  "license": "MIT",
  "config": {},
  "scripts": {
    "debug": "webpack-dev-server --config ./webpack-dev.config.js --hot --open",
    "build-dev": "webpack --config ./webpack-dev.config.js",
    "clean": "rimraf ./lib && mkdir lib",
    "build": "node scripts/build.js",
    "clean:exampledist": "rimraf ./exampledist/static",
    "build:exampledist": "npm run clean:exampledist && cpy src/css/style.css exampledist/static && cross-env NODE_ENV=production webpack --config ./webpack/webpack.config.js",
    "example": "webpack-dev-server --config ./webpack/webpack-dev.config.js --hot",
    "analyz": "cross-env NODE_ENV=production npm_config_report=true npm run build",
    "build:dll": "cross-env NODE_ENV=production webpack --config ./webpack/webpack.dll.config.js"
  },
  "pre-commit": [
    "lint"
  ],
  "dependencies": {
    "antd": "^3.16.6",
    "cpy": "8.1.0",
    "moment": "^2.24.0",
    "prop-types": "^15.7.2",
    "react": "16.13.1",
    "react-dnd": "10.0.2",
    "react-dnd-html5-backend": "10.0.2",
    "react-dom": "16.13.1",
    "rrule": "2.6.4"
  },
  "devDependencies": {
    "@types/moment": "^2.13.0",
    "@types/node": "13.9.5",
    "@types/qs": "6.9.1",
    "@types/react": "16.9.27",
    "@types/react-dnd": "^3.0.2",
    "@types/react-dnd-html5-backend": "^3.0.2",
    "@types/react-dom": "16.9.5",
    "@typescript-eslint/eslint-plugin": "2.25.0",
    "@typescript-eslint/parser": "2.25.0",
    "babel": "6.23.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "10.1.0",
    "babel-loader": "8.1.0",
    "babel-plugin-transform-decorators-legacy": "1.3.5",
    "babel-polyfill": "^6.26.0",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-react": "6.24.1",
    "babel-preset-stage-0": "6.24.1",
    "cross-env": "7.0.2",
    "css-loader": "3.4.2",
    "eslint": "6.8.0",
    "eslint-config-standard": "14.1.1",
    "eslint-plugin-import": "2.20.2",
    "eslint-plugin-node": "11.1.0",
