{
  "compilerOptions": {
    "allowSyntheticDefaultImports": true, //moment
    "jsx": "react",
    "module": "commonjs",
    "noImplicitAny": false,
    "alwaysStrict": true,
    "skipLibCheck": true,
    "outDir": "./lib/",
    "preserveConstEnums": true,
    "removeComments": true,
    "sourceMap": true,
    "target": "es5",
    "forceConsistentCasingInFileNames": true,
    "strict": false,
    "moduleResolution": "node",
    "rootDir": ".",
    "baseUrl": "."
  }
}
